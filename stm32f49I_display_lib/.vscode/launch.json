{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug: app f429i_rect (OpenOCD)",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "cwd": "${workspaceFolder}",
      "executable": "${workspaceFolder}/apps/f429i_rect_app/target/thumbv7em-none-eabihf/debug/f429i_rect_app",
      "preLaunchTask": "build-app-f429i-rect",
      "configFiles": ["interface/stlink.cfg", "target/stm32f4x.cfg"],
      "searchDir": ["/opt/homebrew/share/openocd/scripts"],
      "serverpath": "openocd",
      "gdbPath": "arm-none-eabi-gdb",
      "runToMain": true,
      "postLaunchCommands": [
        "directory ${workspaceFolder}",
        "break main"
      ]
    },
